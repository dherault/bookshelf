{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-relay": "^1.6.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "node ./scripts/setup.js && react-scripts start",
    "build": "node ./scripts/setup.js && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "relay": "node_modules/.bin/relay-compiler --src ./src --schema ../shared/schema.graphql",
    "dev-relay": "npm run relay --watch"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.6.0",
    "graphql": "^0.13.2",
    "relay-compiler": "^1.6.0"
  }
}
